<p id="notice"><%= notice %></p>

<h1>Listing Products</h1
<div id="searched">
  <%= render 'store/index' %>
</div>
<%= link_to "New Product", new_product_path, class: "links" %>
<%= form_tag(search_path, method: "get") do %>
  <%= label_tag(:q, "Search For:") %>
  <%= text_field_tag (:q) %>
  <%= submit_tag("Search") %>
<% end -%>